<?xml version="1.0" encoding="utf-8"?>
<!-- $Id: object.xml 996 2008-10-03 01:58:40Z eddieajau $ -->
<form>
	<fields>
		<field
			name="id"
			type="hidden"
			id="id"
			class="readonly"
			label="Id"
			size="10"
			default="0"
			required="true"
			readonly="true" />
		<field
			name="section_value"
			type="list_sections"
			id="section_value"
			class="inputbox"
			label="JX Section Value"
			size="30"
			default="" />
		<field
			name="name"
			type="text"
			id="name"
			class="inputbox"
			label="JX Object Name"
			size="30"
			default=""
			description="JX Object Name Desc" />
		<field
			name="value"
			type="text"
			id="value"
			class="inputbox"
			label="JX Object Value"
			size="30"
			default=""
			description="JX Object Value Desc" />
		<field
			name="order_value"
			type="text"
			id="order_value"
			class="inputbox"
			label="JX Ordering"
			size="6"
			default="0" />
		<field
			name="hidden"
			type="radio"
			id="hidden"
			class="inputbox"
			label="JX Hidden"
			size="6"
			default="0">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</field>
	</fields>
</form>