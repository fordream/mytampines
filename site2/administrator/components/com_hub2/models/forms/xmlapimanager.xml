<?xml version="1.0" encoding="UTF-8"?>
<!-- xmlapimanager.xml $Id: $ -->
<form>
	<fields>
        <field name="metadata" type="text" id="metadata" class="inputbox validate required"
            label="ADMIN_SITE_XMLAPI_NAME"
            default=""
            description="ADMIN_SITE_XMLAPI_NAME_DESC"
            required="true">
        </field>
		<field name="key_desc" type="text" id="key_desc" class="inputbox validate required"
			label="ADMIN_SITE_XMLAPI_KEY"
			default=""
			description="ADMIN_SITE_XMLAPI_KEY_DESC"
			required="true">
		</field>
		<field name="exp_date" type="text" id="exp_date" class="inputbox required"
			label="ADMIN_SITE_XMLAPI_EXP_DATE"
			default=""
			description="ADMIN_SITE_XMLAPI_EXP_DATE_DESC" 
			required="true">
		</field>
		<field name="user_id" type="list_userlist" id="user_id" class="inputbox validate required @lbl"
		    label="ADMIN_SITE_XMLAPI_USER"
		    size="52"
		    default=""
		    description="ADMIN_SITE_XMLAPI_USER_DESC">
		</field>
		<field name="published" type="list" default="1" label="ADMIN_SITE_XMLAPI_STATUS" description="ADMIN_SITE_XMLAPI_STATUS_DESC">
			  <option value="1">Publish</option>
			  <option value="0">Unpublish</option>
		</field>
	</fields>
</form>
